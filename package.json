{
	"name": "qbp-main",
	"version": "1.2.3",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "env-cmd -f .env.production nest start",
		"start:development": "env-cmd -f .env.development nest start",
		"start:dev": "env-cmd -f .env.development nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "NODE_ENV=production node ./dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"migration:up": "NODE_ENV=development yarn build && yarn run typeorm migration:run --config dist/config/typeorm.config.js",
		"migration:down": "NODE_ENV=development yarn build && yarn run typeorm migration:revert --config dist/config/typeorm.config.js ",
		"migration:create": "NODE_ENV=development yarn build && yarn run typeorm migration:create --config dist/config/typeorm.config.js -n",
		"migration:generate": "NODE_ENV=development yarn build && yarn run typeorm migration:generate --config dist/config/typeorm.config.js -n",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"test:e2e:watch": "jest --config ./test/jest-e2e.json --watch --detectOpenHandles",
		"test:clearCache": "jest --clearCache"
	},
